package com.bridgelabz.model;

public class Student {
	String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public Student()
	{
		System.out.println("Object created......");
	}
	
	public void display(String name)
	{
		System.out.println("Hi...."+name);
	}

}
